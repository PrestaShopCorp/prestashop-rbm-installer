{
  "name": "prestashopcorp/module-lib-billing",
  "type": "library",
  "description": "Utility package to retrieve Built for PS context and more",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PrestaShopCorp\\Billing\\": "src/"
    },
    "classmap": [
      "src/"
    ]
  },
  "require": {
    "php": ">=5.6"
  },
  "require-dev": {
    "prestashop/php-dev-tools": "^5.0",
    "friendsofphp/php-cs-fixer": "^2.19",
    "phpstan/phpstan": "^2.1",
    "prestashop/header-stamp": "^2.3",
    "prestashop/autoindex": "^2.1",
    "squizlabs/php_codesniffer": "^3.11"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
